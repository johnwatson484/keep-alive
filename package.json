{
  "name": "keep-alive",
  "version": "1.1.5",
  "description": "Keep service endpoints active to prevent cool down",
  "main": "app/index.js",
  "type": "module",
  "scripts": {
    "start:watch": "nodemon --inspect=0.0.0.0 --ext js --legacy-watch app/index.js",
    "start:debug": "nodemon --inspect-brk=0.0.0.0 --ext js --legacy-watch app/index.js"
  },
  "author": "John Watson",
  "license": "MIT",
  "dependencies": {
    "axios": "1.9.0"
  },
  "devDependencies": {
    "eslint": "9.26.0",
    "neostandard": "0.12.1",
    "nodemon": "3.0.1"
  },
  "nodemonConfig": {
    "ignore": [
      "**/test-output/**"
    ]
  }
}
